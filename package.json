        "eslint-plugin-react": "^7.12.4",
        "jest": "^24.7.1",
        "metro-react-native-babel-preset": "^0.53.1",
        "prettier": "^1.17.0",
        "prettier-check": "^2.0.0",
        "prop-types": "^15.7.2",
