        "@semantic-release/git": "^7.0.7",
        "@semantic-release/npm": "^5.1.3",
        "@semantic-release/release-notes-generator": "^7.1.4",
        "babel-eslint": "^10.0.1",
        "babel-jest": "^24.7.1",
        "commitizen": "^3.1.1",
        "cz-conventional-changelog": "^2.1.0",
        "eslint": "^5.16.0",
        "eslint-config-prettier": "^4.1.0",
        "eslint-plugin-jest": "^22.4.1",
